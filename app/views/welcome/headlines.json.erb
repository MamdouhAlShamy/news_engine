{"headlines":
             [ 
               <% @headlines.each_with_index do |story,index| %>
	           { "id": <%= story.id.to_json.html_safe %>
	           , "providerId": <%= story.provider_id.to_json.html_safe %>
	           , "categoryId": <%= story.category_id.to_json.html_safe %>
	           , "img": <%= story.img.to_json.html_safe %>
	           , "title": <%= story.title.to_json.html_safe %>
	           }<% unless index== @headlines.count - 1%>,
    <% end %>
    <% end %>
              ]
}
